
public class Aplicacao {

	public static void main(String[] args) {
		exemploWhile();
	}
	
	public static void exemploWhile() {
		int x = 0;
		
		while(true) {
			x++;
			
			if(x==5) {
				continue; // força a execução voltar para a condição do bloco.
			}
			
			if(x==10) {
				break;
			}
			
			System.out.println(x);
		}
	}
	
}
